
main : server.o server_init.o server_deal.o MYSQL.o
	gcc server.o server_init.o  server_deal.o MYSQL.o -o main -L/usr/lib/mysql -lmysqlclient -lpthread
 
server.o : server.c server_init.h
	gcc -c server.c server_init.h
server_init.o : server_init.c server_deal.h MYSQL.h
	gcc -c server_init.c
server_deal.o : server_deal.c MYSQL.h
	gcc -c server_deal.c
MYSQL.o : MYSQL.c dfi.h
	gcc -c MYSQL.c 

